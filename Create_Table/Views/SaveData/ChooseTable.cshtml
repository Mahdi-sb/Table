@{
    ViewData["Title"] = "ChooseTable";
    var Tables = ViewData["Table"] as List<Tables>;

}

<h1>جدول ها </h1>

@foreach (var item in Tables)
{
    <div class="card mb-3 ">
        <div class="card-body" style="background-color:aqua">
            <h5 class="card-title">نام جدول : @(item.TableName) </h5>
        </div>
                <div class="card-footer font-weight-bold text-black-50">
                    <a asp-controller="SaveData" asp-action="SeedData" asp-route-id=@item.Id  class="btn btn-warning"> اضافه کردن داده</a>
                </div>
            
      
        
    </div>
}
